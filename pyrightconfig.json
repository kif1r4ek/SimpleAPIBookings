{
  "pythonVersion": "3.12",
  "pythonPlatform": "Windows",

  "venvPath": ".",
  "venv": ".venv",

  "include": [
    "app",
    "tests"
  ],
  "exclude": [
    "**/__pycache__",
    ".venv",
    "migrations",
    "alembic",
    ".pytest_cache"
  ],

  "typeCheckingMode": "basic",

  "reportMissingImports": "warning",
  "reportMissingTypeStubs": "none",

  "reportUnusedImport": "warning",
  "reportUnusedVariable": "none",
  "reportUnusedCoroutine": "warning"
}
